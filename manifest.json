{
  "manifest_version": 3,
  "name": "YouTube Manager",
  "version": "1.0",
  "description": "Manage YouTube Watch Later playlist and Subscriptions",
  "action": {
    "default_title": "YouTube Manager"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/playlist*",
        "https://www.youtube.com/feed/channels*"
      ],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}