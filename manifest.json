{
  "manifest_version": 3,
  "name": "YouTube Watch Later Manager",
  "version": "1.0",
  "description": "Bulk manage videos in YouTube Watch Later playlist",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ]
}